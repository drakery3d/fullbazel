{
  "$schema": "https://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "$schema": {
      "type": "string"
    },
    "tokens": {
      "type": "object",
      "properties": {
        "auth": {
          "type": "string"
        }
      },
      "required": ["auth"],
      "additionalProperties": false
    },
    "mysqlDatabase": {
      "type": "object",
      "properties": {
        "password": {
          "type": "string"
        }
      },
      "required": ["password"],
      "additionalProperties": false
    },
    "googleClientSecret": {
      "type": "string"
    },
    "vapidPrivateKey": {
      "type": "string"
    },
    "kafka": {
      "type": "object",
      "properties": {
        "apiSecret": {"type": "string"}
      },
      "additionalProperties": false,
      "required": ["apiSecret"]
    }
  },
  "required": ["tokens", "mysqlDatabase", "googleClientSecret", "vapidPrivateKey", "kafka"],
  "additionalProperties": false
}
